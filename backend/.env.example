# Backend environment variables example
# Copy this file to .env and fill the values (do NOT commit real secrets)

# MongoDB connection string (MongoDB Atlas or local)
# Example for Atlas: mongodb+srv://<user>:<password>@cluster0.mongodb.net/mydbname
MONGODB_URI=your_mongodb_connection_string_here

# Optional: specify a DB name to override/ensure a specific DB
MONGODB_DB=ResumeBuilder

# JWT secret used for signing tokens
JWT_SECRET=your_jwt_secret_here

# Backend port
PORT=4000
